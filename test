local me = peripheral.find("meBridge") or error("No ME bridge found!")

print("Does listItems work?")
local items = me.listItems()
print(items)

print("Craftable list:")
print(me.isItemCraftable({name="minecraft:stone_bricks"}))

print("Export test:")
local r = me.exportItem({name="minecraft:stone_bricks", count=1}, "back")
print("Export returned:", r)
